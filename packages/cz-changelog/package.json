{
	"name": "@fat-fuzzy/cz-changelog",
	"package": "@fat-fuzzy/cz-changelog",
	"version": "0.1.8",
	"description": "Commitizen adapter ADAPTED FROM the conventional-changelog format.",
	"main": "index.js",
	"scripts": {
		"commit": "git-cz",
		"test:tofix": "mocha *.test.js",
		"test": "echo \"@fat-fuzzy/cz-changelog: no tests specified\"",
		"format": "prettier --write *.js",
		"semantic-release": "semantic-release"
	},
	"homepage": "https://github.com/fat-fuzzy/rocks/tree/main/packages/cz-changelog",
	"repository": {
		"type": "git",
		"url": "https://github.com/fat-fuzzy/rocks.git"
	},
	"engineStrict": true,
	"engines": {
		"node": ">= 22"
	},
	"author": "Jim Cummins <jimthedev@gmail.com>",
	"license": "MIT",
	"peerDependencies": {
		"@eslint/compat": "^1.0.0"
	},
	"dependencies": {
		"chalk": "5.6.2",
		"commitizen": "4.3.1",
		"conventional-commit-types": "3.0.0",
		"lodash.map": "4.6.0",
		"longest": "2.0.1",
		"word-wrap": "1.2.5"
	},
	"devDependencies": {
		"@fat-fuzzy/config": "workspace:*",
		"@types/chai": "5.2.2",
		"@types/mocha": "10.0.10",
		"chai": "6.0.1",
		"cosmiconfig": "9.0.0",
		"mocha": "11.7.1",
		"mock-require": "3.0.3",
		"semantic-release": "24.2.7",
		"semver": "7.7.2"
	},
	"optionalDependencies": {
		"@commitlint/load": "19.8.1"
	},
	"config": {
		"commitizen": {
			"path": "./index.js"
		}
	}
}
