{
	"compilerOptions": {
		"module": "esnext",
		"target": "esnext",
		"moduleResolution": "bundler",
		"strict": true,
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"declaration": true,
		"skipLibCheck": true,
		"sourceMap": true
	}
}
