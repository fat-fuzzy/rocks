{
	"layout": {
		"$type": "dimension",
		"base": {
			"$value": "1rem"
		},
		"gap": {
			"2xs": {
				"$value": "{scale.base.03}"
			},
			"xs": {
				"$value": "{scale.base.02}"
			},
			"sm": {
				"$value": "{scale.base.01}"
			},
			"md": {
				"$value": "{scale.base.00}"
			},
			"lg": {
				"$value": "{scale.base.10}"
			},
			"xl": {
				"$value": "{scale.base.20}"
			},
			"2xl": {
				"$value": "{scale.base.30}"
			}
		},
		"sidebar": {
			"base": {
				"$value": "24ch"
			},
			"hidden": {
				"$value": "0"
			},
			"collapsed": {
				"$value": "6.5em"
			},
			"2xs": {
				"$value": "10ch"
			},
			"xs": {
				"$value": "12ch"
			},
			"sm": {
				"$value": "18ch"
			},
			"md": {
				"$value": "{layout.sidebar.base}"
			},
			"lg": {
				"$value": "30ch"
			},
			"xl": {
				"$value": "36ch"
			},
			"2xl": {
				"$value": "46ch"
			},
			"3xl": {
				"$value": "60ch"
			}
		},
		"grid": {
			"item": {
				"2xs": {
					"$value": "5rem"
				},
				"xs": {
					"$value": "10rem"
				},
				"sm": {
					"$value": "15rem"
				},
				"md": {
					"$value": "20rem"
				},
				"lg": {
					"$value": "25rem"
				},
				"xl": {
					"$value": "30rem"
				},
				"2xl": {
					"$value": "35rem"
				}
			}
		},
		"breakpoint": {
			"2xs": {
				"$value": "320px"
			},
			"xs": {
				"$value": "620px"
			},
			"sm": {
				"$value": "935px"
			},
			"md": {
				"$value": "1360px"
			},
			"lg": {
				"$value": "1620px"
			},
			"xl": {
				"$value": "2048px"
			},
			"2xl": {
				"$value": "3048px"
			}
		},
		"threshold": {
			"3xs": {
				"$value": "{layout.sidebar.collapsed}"
			},
			"2xs": {
				"$value": "{layout.breakpoint.2xs}"
			},
			"xs": {
				"$value": "{layout.breakpoint.xs}"
			},
			"sm": {
				"$value": "{layout.breakpoint.sm}"
			},
			"md": {
				"$value": "{layout.breakpoint.md}"
			},
			"lg": {
				"$value": "{layout.breakpoint.lg}"
			},
			"xl": {
				"$value": "{layout.breakpoint.xl}"
			},
			"2xl": {
				"$value": "{layout.breakpoint.2xl}"
			}
		},
		"z-index": {
			"0": {
				"$value": "0"
			},
			"1": {
				"$value": "1"
			},
			"2": {
				"$value": "2"
			},
			"3": {
				"$value": "3"
			},
			"4": {
				"$value": "4"
			},
			"5": {
				"$value": "5"
			},
			"inset": {
				"$value": "{layout.z-index.0}"
			},
			"layer": {
				"$value": "{layout.z-index.1}"
			},
			"sticky": {
				"$value": "{layout.z-index.2}"
			},
			"reveal": {
				"$value": "{layout.z-index.3}"
			},
			"fixed": {
				"$value": "{layout.z-index.4}"
			},
			"overlay": {
				"$value": "{layout.z-index.5}"
			}
		}
	},
	"overlay": {
		"$type": "dimension",
		"height": {
			"3xs": {
				"$value": "calc(14vh - var(--ui-size))"
			},
			"2xs": {
				"$value": "calc(20vh - var(--ui-size))"
			},
			"xs": {
				"$value": "calc(30vh - var(--ui-size))"
			},
			"sm": {
				"$value": "calc(40vh - var(--ui-size))"
			},
			"md": {
				"$value": "calc(60vh - var(--ui-size))"
			},
			"lg": {
				"$value": "calc(80vh - var(--ui-size))"
			},
			"xl": {
				"$value": "calc(100vh - var(--ui-header-height) - var(--ui-size) - var(--outline-offset-lg))"
			}
		}
	}
}
