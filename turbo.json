{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": [
				"^build"
			],
			"outputs": [
				".svelte-kit/**",
				".wrangler/**",
				"dist/**",
				"package/**"
			]
		},
		"test": {
			"dependsOn": [
				"^build",
				"^test"
			],
			"outputs": [
				"tests/e2e/test-results/**"
			]
		},
		"lint": {
			"outputs": []
		},
		"dev": {
			"cache": false
		},
		"clean": {
			"cache": false
		}
	}
}
